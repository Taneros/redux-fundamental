{
  "name": "redux-easy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "json-server db.json",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
